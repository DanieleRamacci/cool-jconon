<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="jconon_call_constraint_elenco_macroarea_dipartimentale:jconon_callModel"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 modelSchema.xsd">
	<description>JCONON Dynamic Model</description>
	<author>marco.spasiano</author>
	<version>1.6</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>
	<namespaces>
		<namespace uri="http://www.cnr.it/model/jconon_call_constraint_elenco_macroarea_dipartimentale/cmis" prefix="jconon_call_constraint_elenco_macroarea_dipartimentale" />
	</namespaces>
	<constraints>
		<constraint name="jconon_call_constraint_elenco_macroarea_dipartimentale:values" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Scienze biomediche</value>
					<value>Scienze fisiche e Tecnologie della Materia</value>
					<value>Scienze del sistema terra e tecnologie per l'ambiente</value>
					<value>Scienze chimiche e Tecnologie dei materiali</value>
					<value>Ingegneria, ICT e Tecnologie per l'energia e i trasporti</value>
					<value>Scienze Bio-Agroalimentari</value>
					<value>Scienze umane e sociali, Patrimonio Culturale</value>
				</list>
			</parameter>
		</constraint>
	</constraints>
</model>
