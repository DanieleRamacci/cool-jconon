          xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">

	<fieldProperty
			inputType="TEXT"
			label="label.jconon_attachment_data_inizio"
			name="data_inizio"
			property="jconon_attachment:data_inizio"
			format="DD/MM/YYYY HH:mm:ss"
			widget="ui.datetimepicker">
		<jsonvalidator requiredWidget="true"/>
	</fieldProperty>
	<fieldProperty
			inputType="TEXT"
			label="label.jconon_attachment_data_fine"
			name="data_fine"
			property="jconon_attachment:data_fine"
			format="DD/MM/YYYY HH:mm:ss"
			widget="ui.datetimepicker">
		<jsonvalidator requiredWidget="true"/>
	</fieldProperty>

    <formFieldProperty name="data_inizio"/>
    <formFieldProperty name="data_fine"/>

    <columnFieldProperty name="data_inizio"/>
