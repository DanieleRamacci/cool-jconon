<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="jconon_application_constraint_elenco_possesso_cittadinanza:jconon_callModel"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 modelSchema.xsd">
	<description>JCONON Dynamic Model</description>
	<author>marco.spasiano</author>
	<version>1.6</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>
	<namespaces>
		<namespace uri="http://www.cnr.it/model/jconon_application_constraint_elenco_possesso_cittadinanza/cmis" prefix="jconon_application_constraint_elenco_possesso_cittadinanza" />
	</namespaces>
	<constraints>
		<constraint name="jconon_application_constraint_elenco_possesso_cittadinanza:values" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>italiana</value>					
					<value>di uno stato membro dell'UE</value>
					<value>di essere familiare di un cittadino di uno degli Stati membri della UE non in possesso della cittadinanza di uno stato membro della UE ma titolare del diritto di soggiorno o di soggiorno permanente</value>
					<value>di un paese terzo in possesso del permesso di soggiorno CE per soggiornanti di lungo periodo</value>
					<value>rifugiato ovvero dello stato di protezione sussidiaria</value>
				</list>
			</parameter>
		</constraint>		
	</constraints>
</model>
